{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "dist/"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }